{
  "name": "@adonisjs/antl",
  "version": "2.0.7",
  "description": "Localization for adonisjs app",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "npm run lint",
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc node japaFile.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "commit": "git-cz",
    "lint": "standard",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "keywords": [
    "adonis",
    "adonisjs",
    "adonis-intl",
    "intl",
    "localization"
  ],
  "author": "virk",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/mrm-preset": "^2.0.3",
    "@adonisjs/sink": "^1.0.17",
    "commitizen": "^3.1.1",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "japa": "^2.0.3",
    "japa-cli": "^1.0.1",
    "mrm": "^1.2.1",
    "nyc": "^14.1.1",
    "pkg-ok": "^2.2.0",
    "semver": "^6.2.0",
    "standard": "^12.0.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "standard": {
    "globals": [
      "use"
    ]
  },
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "dependencies": {
    "@adonisjs/generic-exceptions": "^2.0.1",
    "debug": "^4.0.1",
    "flat": "^4.1.0",
    "intl": "^1.2.5",
    "intl-format-cache": "^2.1.0",
    "intl-messageformat": "^5.0.1",
    "intl-relativeformat": "^2.1.0",
    "lodash": "^4.17.11",
    "require-all": "^3.0.0"
  },
  "devDependencies": {
    "snyk": "^1.189.0"
  },
  "snyk": true
}
